{
  "name": "webpack-test",
  "version": "1.0.0",
  "description": "webpack-test",
  "scripts": {
    "build:vendor": "webpack --config vendor/webpack.config.js",
    "build:shared": "webpack --config shared/webpack.config.js",
    "build:app1": "webpack --config app1/webpack.config.js",
    "build:app2": "webpack --config app2/webpack.config.js",
    "build:apps": "npm run build:app1 && npm run build:app2",
    "build:all": "npm run build:vendor && npm run build:shared && npm run build:apps",
    "start": "node server.js"
  },
